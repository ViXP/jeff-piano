{
  "presets": [
    [
      "@babel/env",
      {
        "modules": false,
        "targets": {
          "browsers": "> 1%",
          "uglify": true
        },
        "useBuiltIns": "entry"
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "syntax-dynamic-import",
    ["transform-object-rest-spread", {
      "useBuiltIns": true
    }],
    [
      "transform-class-properties",
      {
        "spec": true
      }
    ]
  ]
}
